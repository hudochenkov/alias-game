$default-font: -apple-system, BlinkMacSystemFont, "Segoe UI", "Roboto", "Oxygen", "Ubuntu", "Cantarell", "Fira Sans", "Droid Sans", "Helvetica Neue", sans-serif;
$default-color: #333;

@custom-media --palm (width < 768px);
@custom-media --not-palm (768px <= width);
@custom-media --lap (768px <= width <= 1024px);
@custom-media --portable (width <= 1024px);
@custom-media --desk (1024px < width);
